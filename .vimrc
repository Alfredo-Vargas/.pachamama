syntax on
filetype plugin indent on
set noerrorbells
set tabstop=4 softtabstop=4
set shiftwidth=4
set expandtab
set guifont=Iosevka\ 20
set guioptions-=m
set guioptions-=T
set noesckeys
set number
set ignorecase
set smartcase
set incsearch
set nohlsearch
set autochdir
set cinoptions=l1
colorscheme elflord
inoremap jk <ESC>
let g:mapleader = "\<Space>"

